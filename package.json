{
  "name": "scanner",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "22.x.x"
  },
  "scripts": {
    "build": "nest build",
    "build:all": "concurrently \"npm run build:api\" \"npm run build scan-engine\" \"npm run build cli\"",
    "build:api": "nest build api",
    "build:cli": "nest build cli",
    "build:scan-engine": "nest build scan-engine",
    "format": "prettier --write \"apps/**\" \"libs/**\"",
    "ingest": "nest start cli -- ingest",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "rmdist": "rimraf dist",
    "scan": "nest start cli -- scan-site --url supremecourt.gov",
    "scan:dap": "nest start cli --watch -- scan-site --url supremecourt.gov --page primary --scan dap",
    "scan:dev": "npm run scan:dev:scourt",
    "scan:dev:scourt": "nest start cli --watch -- scan-site --url supremecourt.gov",
    "scan:dev:rdcp": "nest start cli --watch -- scan-site --url rdcp.cdc.gov",
    "scan:badlogs": "nest start cli --watch -- scan-site --url rdcp.cdc.gov | grep -v '\"project\":\"site-scanning\"'",
    "scan:badurl": "nest start cli --watch -- scan-site --url an.unknown.url",
    "snapshot": "nest start cli -- create-snapshot",
    "start": "nest start",
    "start:all": "concurrently \"npm run start:api\" \"npm run start:concurrent-scan-engine\"",
    "start:api": "nest start api",
    "start:concurrent-scan-engine": "wait-on http://localhost:3000/api-json && nest start scan-engine",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod:api": "node dist/apps/api/main.js",
    "start:prod:scan-engine": "node dist/apps/scan-engine/main.js",
    "start:scan-engine": "nest start scan-engine",
    "tdd": "jest --testPathIgnorePatterns e2e-spec --watch",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest e2e-spec",
    "test:unit": "jest --testPathIgnorePatterns e2e-spec",
    "test:watch": "jest --watch --testPathIgnorePatterns e2e-spec"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.621.0",
    "@axe-core/puppeteer": "^4.10.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.7",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^8.0.5",
    "@nestjs/typeorm": "^10.0.2",
    "@types/json2csv": "^5.0.1",
    "bull": "^4.16.4",
    "class-transformer": "^0.5.1",
    "class-validator": ">=0.14.0",
    "commander": "^12.1.0",
    "cuid": "^3.0.0",
    "fast-csv": "^5.0.2",
    "generic-pool": "^3.8.2",
    "http-status-codes": "^2.2.0",
    "json2csv": "^5.0.7",
    "lodash": "^4.17.21",
    "nestjs-pino": "^4.1.0",
    "nestjs-typeorm-paginate": "^4.0.3",
    "pg": "^8.7.3",
    "pino-http": "^6.6.0",
    "pino-pretty": "^13.0.0",
    "puppeteer": "^23.8.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.5.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.14",
    "uuid": "^11.0.3",
    "webpack": ">=5.94.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.3.7",
    "@types/bull": "^3.15.7",
    "@types/express": "^5.0.0",
    "@types/generic-pool": "^3.1.10",
    "@types/html_codesniffer": "^2.5.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.178",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.4",
    "globals": "^15.12.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.3",
    "mermaid": "^11.6.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sqlite3": "^5.1.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "wait-on": "^8.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testTimeout": 10000,
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/entities/",
      "<rootDir>/libs/"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "@app/message-queue/(.*)": "<rootDir>/libs/message-queue/src/$1",
      "@app/message-queue": "<rootDir>/libs/message-queue/src",
      "@app/database/(.*)": "<rootDir>/libs/database/src/$1",
      "@app/database": "<rootDir>/libs/database/src",
      "@app/browser/(.*)": "<rootDir>/libs/browser/src/$1",
      "@app/browser": "<rootDir>/libs/browser/src",
      "@app/core-scanner/(.*)": "<rootDir>/libs/core-scanner/src/$1",
      "@app/core-scanner": "<rootDir>/libs/core-scanner/src",
      "@app/ingest/(.*)": "<rootDir>/libs/ingest/src/$1",
      "@app/ingest": "<rootDir>/libs/ingest/src",
      "@app/storage/(.*)": "<rootDir>/libs/storage/src/$1",
      "@app/storage": "<rootDir>/libs/storage/src",
      "@app/snapshot/(.*)": "<rootDir>/libs/snapshot/src/$1",
      "@app/snapshot": "<rootDir>/libs/snapshot/src",
      "@app/datetime/(.*)": "<rootDir>/libs/datetime/src/$1",
      "@app/datetime": "<rootDir>/libs/datetime/src",
      "@app/queue": "<rootDir>/libs/queue/src",
      "@app/queue/(.*)": "<rootDir>/libs/queue/src/$1",
      "@app/security-data/(.*)": "<rootDir>/libs/security-data/src/$1",
      "@app/security-data": "<rootDir>/libs/security-data/src"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.ts"
    ]
  }
}
